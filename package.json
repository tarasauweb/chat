{
  "name": "chat",
  "private": true,
  "workspaces": [
    "react-app",
    "electron",
    "express"
  ],
  "scripts": {
    "dev:react": "npm --workspace react-app run dev",
    "dev:electron": "npm --workspace electron start",
    "dev:express": "npm --workspace express start",
    "dev:all": "concurrently \"npm run dev:react\" \"npm run dev:electron\" \"npm run dev:express\""
  },
  "devDependencies": {
    "concurrently": "^8.0.0"
  }
}
